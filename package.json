{
  "name": "hc-officesupplies",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "@sap/hana-client": "^2.12.25",
    "@sap/hdbext": "7.6.4",
    "@sap/xsenv": "^3.2.2",
    "@sap/xssec": "^3.2.13",
    "express": "^4",
    "hdb": "^0.18.3",
    "passport": "^0.6.0",
    "sap-hdbext-promisfied": "^2.202301.1"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.100.3",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run",
    "hybrid": "cds watch --profile hybrid"
  },
  "engines": {
    "node": "^16.15"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana-cloud"
      }
    }
  }
}
